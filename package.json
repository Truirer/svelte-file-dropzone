{
  "name": "svelte-file-dropzone",
  "version": "2.0.9",
  "description": "Svelte component for fileupload and file dropzone",
  "scripts": {
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "Dropzone.svelte": [
        "./dist/components/Dropzone.svelte.d.ts"
      ]
    }
  },
  "repository": {
    "url": "https://github.com/thecodejack/svelte-file-dropzone"
  },
  "author": "thecodejack",
  "license": "MIT",
  "dependencies": {
    "file-selector": "^0.6.0"
  },
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0 || ^5"
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.20.4",
    "@sveltejs/package": "^2.3.10",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "babel-loader": "9.1.3",
    "playwright": "^1.51.1",
    "publint": "^0.2.7",
    "svelte": "^5.25.9",
    "vite": "^6.2.5",
    "vitest": "^3.1.1"
  },
  "keywords": [
    "svelte",
    "svelte3",
    "svelte5",
    "svelte-components",
    "upload",
    "dropzone",
    "svelte-file-dropzone",
    "svelte-dropzone",
    "sveltejs"
  ],
  "files": [
    "src",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
